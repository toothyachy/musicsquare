


<div class="container">
  <div class="row">
    <% @listings.each do |listing| %>
      <div class="col-3">
        <div class="listing rounded p-3">
          <%= image_tag listing.images.first %>
          <div class="listing-description mt-4">
            <p><%= listing.name %></p>
            <p><%= listing.liked_genres %></p>
            <p><%= listing.looking_for %></p>
            <p>Sound clip button</p>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>




<%# div class is used generally. %>
